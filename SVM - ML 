    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.svm import SVC\n",
        "\n",
        "# Step 1: Generate quadratic dataset\n",
        "np.random.seed(42)\n",
        "n_samples = 200\n",
        "\n",
        "# Random x values\n",
        "X = np.random.uniform(-3, 3, (n_samples, 1))\n",
        "\n",
        "# Quadratic relationship with some noise\n",
        "y_values = X[:, 0]**2 + np.random.normal(0, 1, n_samples)\n",
        "\n",
        "# Classification target: above/below threshold\n",
        "y = (y_values > 4).astype(int)  # Class 1 if above curve, else 0\n",
        "\n",
        "# Combine into 2D dataset (X and quadratic feature)\n",
        "X_data = np.hstack([X, y_values.reshape(-1, 1)])\n",
        "\n",
        "# Step 2: Train an SVM classifier with RBF kernel (handles nonlinearity)\n",
        "clf = SVC(kernel='rbf', C=1.0, gamma='scale')\n",
        "clf.fit(X_data, y)\n",
        "\n",
        "# Step 3: Plot decision boundary\n",
        "xx, yy = np.meshgrid(np.linspace(-3, 3, 200), np.linspace(-2, 12, 200))\n",
        "grid = np.c_[xx.ravel(), yy.ravel()]\n",
        "Z = clf.predict(grid).reshape(xx.shape)\n",
        "\n",
        "plt.contourf(xx, yy, Z, alpha=0.3, cmap=plt.cm.coolwarm)\n",
        "plt.scatter(X_data[:, 0], X_data[:, 1], c=y, cmap=plt.cm.coolwarm, edgecolors='k')\n",
        "plt.xlabel(\"X\")\n",
        "plt.ylabel(\"Quadratic feature (X^2 + noise)\")\n",
        "plt.title(\"Quadratic Dataset with SVM Decision Boundary\")\n",
        "plt.show()\n"
      ],
